# .env.example - 환경변수 설정 예제
# 이 파일을 복사하여 .env 파일을 만들고 실제 값을 입력하세요

# 서버 설정
NODE_ENV=development
PORT=3001
HOST=localhost

# 특허청 API 설정
KIPRIS_API_KEY=your_kipris_api_key_here
KIPRIS_API_BASE_URL=http://plus.kipris.or.kr/kipo-api/kipi

# 특허청 등록원부 API 설정 (등록특허용)
PATENT_OFFICE_API_KEY=your_patent_office_api_key_here
PATENT_OFFICE_API_URL=https://apis.data.go.kr/1430000/PttRgstRtInfoInqSvc/getBusinessRightList

# 로깅 설정
LOG_LEVEL=debug

# CORS 설정 (콤마로 구분)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,https://unicpat.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100